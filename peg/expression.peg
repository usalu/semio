Expression     <- Operation / Atom
Operation      <- Identifier _ "(" _ ExpressionList? _ ")"
ExpressionList <- Expression (_ Expression)*
Atom           <- Number / Identifier

Number         <- [0-9]+ ("." [0-9]+)? ([eE] [+-]? [0-9]+)?
Identifier     <- [a-zA-Z_][a-zA-Z0-9._-]*

_              <- [ \t\r\n]*
