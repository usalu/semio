}

# ğŸ”Œ A port is a connection point (with a direction) of a type.
type Port {
    # ğŸ’¬ The optional human-readable description of the port.
    description?: string;
    # â¡ï¸ The direction of the port. When another piece connects the direction of the other port is flipped and then the pieces are aligned.
    direction: Vector;
    # ğŸ†” The id of the port.
    id_?: string;
    # ğŸ—ºï¸ The locators of the port.
    locators?: Locator[];
    # âœ–ï¸ The connection point of the port that is attracted to another connection point.
    point: Point;
}

# ğŸ—ºï¸ A locator is meta-data for grouping ports.
