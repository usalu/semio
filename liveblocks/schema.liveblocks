# ğŸ–‡ï¸ A bidirectional connection between two pieces of a design.
type Connection {
    # ğŸ§² The connected side of the connection.
    connected: Side;
    # ğŸ§² The connecting side of the connection.
    connecting: Side;
    # â†•ï¸ The optional longitudinal gap (applied after rotation and tilt in port direction) between the connected and the connecting piece.
    gap?: number;
    # ğŸ”„ The optional horizontal rotation in port direction between the connected and the connecting piece in degrees.
    rotation?: number;
    # â†”ï¸ The optional lateral shift (applied after rotation and tilt in the plane) between the connected and the connecting piece..
    shift?: number;
    # â†—ï¸ The optional horizontal tilt perpendicular to the port direction (applied after rotation) between the connected and the connecting piece in degrees.
    tilt?: number;
    # â¡ï¸ The optional offset in x direction between the icons of the child and the parent piece in the diagram. One unit is equal the width of a piece icon.
    x?: number;
    # â¬†ï¸ The optional offset in y direction between the icons of the child and the parent piece in the diagram. One unit is equal the width of a piece icon.
    y?: number;
}

# ğŸ§± A side of a piece in a connection.
type Side {
    piece: PieceID;
    port: PortID;
}

# ğŸªª The props to identify the piece within the parent design.
type PieceID {
    # ğŸ†” The id of the piece.
    id_?: string;
}

# ğŸªª The props to identify the port within the parent type.
type PortID {
    # ğŸ†” The id of the port.
    id_?: string;
}

# â­• A piece is a 3d-instance of a type in a design.
type Piece {
    # ğŸ“º The optional center of the piece in the diagram. When pieces are connected only one piece can have a center.
    center?: null | DiagramPoint;
    # ğŸ†” The id of the piece.
    id_?: string;
    # â—³ The optional plane of the piece. When pieces are connected only one piece can have a plane.
    plane?: null | Plane;
    # ğŸ§© The type of the piece.
    type: TypeID;
}

# ğŸ“º A 2d-point (xy) of integers in screen coordinate system.
type DiagramPoint {
    # ğŸ The x-coordinate of the icon of the piece in the diagram. One unit is equal the width of a piece icon.
    x: number;
    # ğŸ The y-coordinate of the icon of the piece in the diagram. One unit is equal the width of a piece icon.
    y: number;
}

# â—³ A plane is an origin (point) and an orientation (x-axis and y-axis).
type Plane {
    # âŒ± The origin of the plane.
    origin: Point;
    # â¡ï¸ The x-axis of the plane.
    xAxis: Vector;
    # â¡ï¸ The y-axis of the plane.
    yAxis: Vector;
}

# âœ–ï¸ A 3d-point (xyz) of floating point numbers.
type Point {
    # ğŸšï¸ The x-coordinate of the point.
    x: number;
    # ğŸšï¸ The y-coordinate of the point.
    y: number;
    # ğŸšï¸ The z-coordinate of the point.
    z: number;
}

# â¡ï¸ A 3d-vector (xyz) of floating point numbers.
type Vector {
    # ğŸšï¸ The x-coordinate of the vector.
    x: number;
    # ğŸšï¸ The y-coordinate of the vector.
    y: number;
    # ğŸšï¸ The z-coordinate of the vector.
    z: number;
}

# ğŸªª The props to identify the type.
type TypeID {
    # ğŸ“› The name of the type.
    name: string;
    # ğŸ”€ The optional variant of the type. No variant means the default variant.
    variant?: string;
}

# â†—ï¸ The output of a quality.
type Quality {
    # ğŸ“ The optional definition [ text | url ] of the quality.
    definition?: string;
    # ğŸ“ The name of the quality.
    name: string;
    # ğŸ“ The optional unit of the value of the quality.
    unit?: string;
    # ğŸ“ The optional value [ text | url ] of the quality. No value is equivalent to true for the name.
    value?: string;
}

# ğŸ§© A type is a reusable element that can be connected with other types over ports.
type Type {
    authors?: Author[];
    # ğŸ•’ The creation string of the type.
    createdAt?: string;
    # ğŸ’¬ The optional human-readable description of the type.
    description?: string;
    # ğŸª™ The optional icon [ emoji | logogram | url ] of the type. The url must point to a quadratic image [ png | jpg | svg ] which will be cropped by a circle. The image must be at least 256x256 pixels and smaller than 1 MB.
    icon?: string;
    # ğŸ–¼ï¸ The optional url to the image of the type. The url must point to a quadratic image [ png | jpg | svg ] which will be cropped by a circle. The image must be at least 720x720 pixels and smaller than 5 MB.
    image?: string;
    # ğŸ•’ The last upstring string of the type.
    lastUpstringAt?: string;
    # ğŸ“› The name of the type.
    name: string;
    ports?: Port[];
    qualities?: Quality[];
    representations?: Representation[];
    # â“‚ï¸ The length unit of the point and the direction of the ports of the type.
    unit?: string;
