    {
      "key": "semio.cost.life-cycle.earnings.roi",
      "name": "Return on Investment",
      "translation": {
        "de": "Rendite"
      },
      "description": "The interest is the Return on Investment (ROI) [%] between the initial investment cost and the net life cycle earnings.",
      "si": "%",
      "imperial": "%",
      "symbol": "ROI",
      "formula": " Divide ( Subtract ( semio.cost.life-cycle.earnings.roi semio.cost.investment.initial ) semio.cost.investment.initial ) ",
      "format": "#,##0.#%"
    },
    {
      "key": "semio.cost.life-cycle.earnings",
      "name": "Life Cycle earnings",
      "translation": {
        "de": "Lebenszyklus-Einnahmen"
      },
      "description": "The life cycle earnings is the total earnings of its income over the life cycle of the building.",
      "si": "€",
      "imperial": "$",
      "symbol": "E",
      "formula": "Multiply ( semio.cost.life-cycle.earnings.life-time semio.cost.life-cycle.earnings.income )",
      "format": "#,##0.##"
    },
    {
      "key": "semio.cost.life-cycle.earnings.life-time",
      "name": "Life Time",
      "translation": {
        "de": "Lebensdauer"
      },
      "description": "The lifetime is the total expected lifespan of the building, expressed in years.",
      "si": "a",
      "imperial": "yr",
      "symbol": "t",
      "formula": "Multiply ( 1 semio.cost.life-cycle.earnings.life-time )",
      "format": "#,##0.##"
